{
  "name": "vault_viewer",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "run-p -l dev:typecheck dev:vite dev:electron",
    "dev:typecheck": "tsc -p tsconfig.electron.json --watch --preserveWatchOutput",
    "dev:vite": "vite",
    "dev:electron": "wait-on tcp:5173 dist/electron/main.js && nodemon -q --watch dist/electron --ext js --delay 100ms --exec \"cross-env ELECTRON_START_URL=http://localhost:5173 electron --inspect=9229 --remote-debugging-port=9223 dist/electron/main.js\"",
    "build": "run-s build:renderer build:main",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.electron.json",
    "start": "electron dist/electron/main.js"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.6",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "wait-on": "^8.0.4"
  }
}
